{
    "extends": "../../tsconfig.json",
    "include": ["src"],
    "exclude": ["./node_modules/", "./dist/"],
    "compilerOptions": {
      "outDir": "dist",
      "skipLibCheck": true,
      "moduleResolution": "Node",
      "useUnknownInCatchVariables": false
    },
    "ts-node": {
      "esm": true,
      "experimentalSpecifierResolution": "node",
      "experimentalResolver": true
    }
  }
  